<section class="sm:my-3 p-6">
  <h2 class="text-2xl">My Wishlist</h2>
  <div class="bg-sky-100 mt-2">
    {{#if products.length}}
    <ul class="existingListings p-2 sm:p-3">
      {{#each products as |product|}}
      {{>result-list product}}
      {{/each}}
    </ul>
    {{/if}}
  </div>
</section>